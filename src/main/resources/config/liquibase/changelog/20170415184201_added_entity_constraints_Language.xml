<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Language.
    -->
    <changeSet id="20170415184201-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="languages_id"
                                 baseTableName="language_stores"
                                 constraintName="fk_language_stores_languages_id"
                                 referencedColumnNames="id"
                                 referencedTableName="language"/>
        <addForeignKeyConstraint baseColumnNames="stores_id"
                                 baseTableName="language_stores"
                                 constraintName="fk_language_stores_stores_id"
                                 referencedColumnNames="id"
                                 referencedTableName="merchant_store"/>
        
    </changeSet>
</databaseChangeLog>
