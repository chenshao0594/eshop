<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity MerchantStore.
    -->
    <changeSet id="20170415184137-1" author="jhipster">
        <createTable tableName="merchant_store">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="storeaddress" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="code" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="storename" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="store_email_address" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="d_efaultstore" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="storephone" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="weightunitcode" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="use_cache" type="bit">
                <constraints nullable="true" />
            </column>

            <column name="store_template" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="domain_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="invoice_template" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="store_logo" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="in_business_since" type="date">
                <constraints nullable="true" />
            </column>

            <column name="currency_format_national" type="bit">
                <constraints nullable="true" />
            </column>

            <column name="storepostalcode" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="seizeunitcode" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="storestateprovince" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="continueshoppingurl" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="storecity" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="created_by" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="created_date" type="date">
                <constraints nullable="true" />
            </column>

            <column name="last_modified_by" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="last_modified_date" type="date">
                <constraints nullable="true" />
            </column>

            <column name="default_language_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="zone_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="currency_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="country_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
